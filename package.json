{
  "name": "y3x",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "build:dev": "vite build --mode=development",
    "build:prod": "vite build --mode=production",
    "preview": "vite build && vite preview",
    "commit": "git-cz",
    "start": "yarn dev & wait-on tcp:3000 -v",
    "test": "cypress open",
    "test-components": "cypress open-ct",
    "test:ci": "cypress run --headless",
    "test:ci-components": "cypress run-ct",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vueuse/core": "^6.7.4",
    "@vueuse/head": "~0.6",
    "axios": "^0.24.0",
    "mockjs": "^1.1.0",
    "vue": "~3.2",
    "vue-router": "~4.0",
    "vuex": "~4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@cypress/vite-dev-server": "~2.2",
    "@cypress/vue": "~3.0",
    "@tailwindcss/aspect-ratio": "~0.3",
    "@tailwindcss/forms": "~0.3",
    "@tailwindcss/line-clamp": "~0.2",
    "@tailwindcss/typography": "~0.4",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "~5",
    "@typescript-eslint/parser": "~5",
    "@vitejs/plugin-vue": "~1.9",
    "@vue/eslint-config-standard": "~6.1",
    "@vue/eslint-config-typescript": "~8",
    "autoprefixer": "~10",
    "commitizen": "^4.2.4",
    "cypress": "~8.7",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "~8.3",
    "eslint-plugin-import": "~2",
    "eslint-plugin-node": "~11.1",
    "eslint-plugin-promise": "~5.1",
    "eslint-plugin-vue": "^8.0.3",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "postcss": "~8.3",
    "postcss-nesting": "~9",
    "prettier": "^2.4.1",
    "tailwindcss": "~2.2",
    "typescript": "~4.4",
    "vite": "~2.6",
    "vite-plugin-mock": "^2.9.6",
    "wait-on": "~6.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}